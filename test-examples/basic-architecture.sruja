specification {
  element person
  element system
  element container
  element component
  element database
  element queue
}

model {
  customer = person "Online Shopper"
  admin = person "System Administrator"
  
  webApp = system "Web Application" {
    frontend = container "React Frontend" {
      shoppingCart = component "Shopping Cart Component"
      productCatalog = component "Product Catalog Component"
    }
    
    api = container "REST API" {
      orderService = component "Order Management"
      paymentService = component "Payment Processing"
    }
    
    db = database "PostgreSQL Database"
    events = queue "Event Queue"
  }
  
  paymentGateway = system "External Payment Gateway" {
    external
  }
  
  customer -> webApp "browses products"
  webApp -> paymentGateway "processes payments"
  admin -> webApp "manages inventory"
}















































